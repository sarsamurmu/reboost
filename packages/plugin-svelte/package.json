    "directory": "packages/plugin-svelte"
  },
  "homepage": "https://github.com/sarsamurmu/reboost/tree/primary/packages/plugin-svelte",
  "author": "Sarsa Murmu <sarsamurmu05@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "svelte": "^3.31.2"
  },
  "devDependencies": {
    "magic-string": "^0.25.7",
    "svelte": "^3.31.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "svelte-hmr": "^0.12.2"
  }
}
