{
  "name": "@reboost/plugin-svelte",
  "version": "0.19.2",
  "description": "Svelte plugin for Reboost",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "node ../../prepare rm-dist",
