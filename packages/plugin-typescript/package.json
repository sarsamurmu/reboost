{
  "name": "@reboost/plugin-typescript",
  "version": "0.20.1",
  "description": "TypeScript plugin for Reboost",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "node ../../prepare rm-dist",
