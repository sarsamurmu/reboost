{
  "name": "reboost",
  "version": "0.11.0",
  "description": "A super fast dev server for rapid web development",
  "main": "./dist/node/index.js",
  "types": "./dist/node/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "node ../../rmDist",
    "build": "tsc -p browser.tsconfig.json && tsc -p node.tsconfig.json",
    "watch": "concurrently \"tsc -p browser.tsconfig.json -w\" \"tsc -p node.tsconfig.json -w\"",
    "test": "mocha -r ts-node/register/transpile-only --recursive tests/**/*.test.ts",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "fast",
    "super-fast",
    "hot-reload",
    "esm",
    "ecmascript-moudles",
    "commonjs-modules",
    "no-bundle",
    "dev-server",
    "web-development",
    "rapid-development"
  ],
  "repository": "github:sarsamurmu/reboost",
  "author": "Sarsa Murmu <sarsamurmu05@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.10.4",
    "@babel/generator": "^7.11.0",
    "@babel/parser": "^7.11.3",
    "@babel/traverse": "^7.11.0",
    "@babel/types": "^7.11.0",
    "@koa/cors": "^3.1.0",
    "@types/babel__generator": "^7.6.1",
    "@types/babel__traverse": "^7.0.13",
    "@types/koa": "^2.11.3",
    "anymatch": "^3.1.1",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.2",
    "enhanced-resolve": "^5.0.0-beta.10",
    "esbuild": "^0.6.21",
    "icss-utils": "^4.1.1",
    "koa": "^2.13.0",
    "koa-proxies": "^0.11.0",
    "koa-send": "^5.0.1",
    "magic-string": "^0.25.7",
    "md5-file": "^5.0.0",
    "node-html-parser": "^1.2.20",
    "open": "^7.1.0",
    "portfinder": "^1.0.28",
    "postcss": "^7.0.32",
    "postcss-load-config": "^2.1.0",
    "postcss-modules-extract-imports": "^2.0.0",
    "postcss-modules-local-by-default": "^3.0.3",
    "postcss-modules-scope": "^2.2.0",
    "postcss-modules-values": "^3.0.0",
    "pretty-time": "^1.1.0",
    "source-map": "^0.7.3",
    "ws": "^7.3.1"
  },
