{
  "npm.exclude": "{**/playground,**/plugin-template,**/create-app/templates/**}",
  "cSpell.words": [
    "escodegen",
    "estree",
    "etag",
    "litcss",
    "malina",
    "malinajs",
    "meriyah"
  ],
  "files.associations": {
    "*.tsconfig.json": "jsonc"
  },
  "json.schemas": [
    {
      "fileMatch": ["*.tsconfig.json"],
      "url": "https://json.schemastore.org/tsconfig"
    }
  ],
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
